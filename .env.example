# LiveAvatar API Key - get from https://app.liveavatar.com/settings
# (Previously HeyGen - the API key is the same if you migrated)
LIVEAVATAR_API_KEY=your_liveavatar_api_key_here

# LiveAvatar Avatar ID - get from https://app.liveavatar.com/avatars
LIVEAVATAR_AVATAR_ID=your_avatar_id_here

# Anthropic API Key - get from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Legacy HeyGen key (still supported as fallback)
# HEYGEN_API_KEY=your_heygen_api_key_here
